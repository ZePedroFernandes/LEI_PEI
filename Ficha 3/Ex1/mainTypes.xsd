<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified"
    xmlns:p="zepedro/pizza">
    <xs:import schemaLocation="pizzaTypes.xsd" namespace="zepedro/pizza"/>

    <xs:element name="menu">
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element name="pizza" type="p:pizza"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
